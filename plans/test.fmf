/pull-request/fedora:
  summary:
    systemd Fedora testsuite
  discover:
    how: fmf
  prepare:
    how: ansible
    playbooks:
      - plans/fedora-test-deps.yml
    exclude:
      - systemd-standalone-tmpfiles
      - systemd-standalone-sysusers
  execute:
    framework: beakerlib
    how: tmt
